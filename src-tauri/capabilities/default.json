{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "fs:allow-write",
    "fs:allow-write-file",
    {
      "identifier": "fs:scope",
      "allow": [
        "**/*"
      ]
    }
  ],
  "allowlist": {
    "fs": {
      "all": true
    }
  }
}
